{
    "name": "josephdpurcell/workspace-preview-system",
    "description": "Get started with the Workspace Preview System",
    "type": "drupal-profile",
    "license": "GPL-2.0+",
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packagist.drupal-composer.org"
        }
    ],
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
    },
    "extra": {
        "installer-paths": {
            "docroot/core": [
                "drupal/core"
            ],
            "modules/contrib/{$name}": [
                "type:drupal-module"
            ]
        }
    },
    "require": {
        "composer/installers": "^1.0.20",
        "cweagans/composer-patches": "~1.0",
        "drupal-composer/drupal-scaffold": "^1.2",
        "drupal/core": "^8.1.0",
        "drush/drush": "dev-master",
        "drupal/multiversion": "^8.1.0",
        "drupal/deploy": "^8.1.0",
        "drupal/workspace": "^8.1.0",
        "drupal/relaxed": "^8.1.0",
        "drupal/diff": "^8.1.0",
        "drupal/workbench_moderation": "^8.1.0",
        "drupal/trash": "^8.1.0"
    }
}
