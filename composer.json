{
    "name": "josephdpurcell/workspace_preview_system",
    "description": "Get started with the Workspace Preview System",
    "type": "drupal-profile",
    "license": "GPL-2.0+",
    "minimum-stability": "dev",
    "require-dev": {
        "phing/phing": "^2.14"
    },
    "config": {
        "bin-dir": "bin/"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packagist.drupal-composer.org"
        }
    ],
    "scripts": {
        "post-install-cmd": [
            "./bin/phing"
        ],
        "post-update-cmd": [
            "./bin/phing push"
        ],
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
    },
    "extra": {
        "installer-paths": {
            "docroot/core": [
                "drupal/core"
            ],
            "modules/contrib/{$name}": [
                "type:drupal-module"
            ]
        }
    },
    "require": {
        "composer/installers": "^1.0.20",
        "cweagans/composer-patches": "~1.0",
        "drupal-composer/drupal-scaffold": "^1.3.1",
        "drupal/core": "^8.2.0",
        "drush/drush": "^9.0",
        "drupal/multiversion": "dev-8.x-1.x",
        "drupal/workspace": "^8.1",
        "drupal/workbench_moderation": "^8.1"
    }
}
